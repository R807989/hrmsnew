pipeline {
agent any
stages{

stage("intigration") {


steps{



git branch: 'main', url: 'https://github.com/R807989/hrmsnew.git'
}

}


stage('install python depencies') {
            steps {
                script {
                    // Install dependencies using Pipenv
                    sh 'pipenv install --dev'
                }
            }
        }




stage("build image") {

steps {



dir("/var/lib/jenkins/workspace/abdul autoamtion/HRMS-Server"){

script {
 sh 'docker build -t abdul' .

}

}

}
}



}




}
